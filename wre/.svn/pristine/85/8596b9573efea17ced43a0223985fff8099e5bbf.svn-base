<span class="digits_10 code_line"></span>&lt;/w2:gridView&gt;
<span class="digits_10 code_line"></span>
<span class="digits_10 code_line"></span><span class="r_h_comment_span">&lt;!-- gridView 의 소스 본문 예시 - 컬럼에 속성을 지정 --&gt;</span>
<span class="digits_10 code_line"></span>&lt;w2:gridView <span class="r_h_attribute_name_span">editModeEventIcon</span>=<span class="r_h_attribute_value_span">"onclick"</span> <span class="r_h_attribute_name_span">dataList</span>=<span class="r_h_attribute_value_span">"data:dlt_books"</span> <span class="r_h_attribute_name_span">style</span>=<span class="r_h_attribute_value_span">"height: 100px;"</span>&gt;
<span class="digits_10 code_line"></span>&nbsp;&nbsp;&nbsp;&nbsp;<span class="r_h_comment_span">&lt;!-- 중략 --&gt;</span>
<span class="digits_10 code_line"></span>&nbsp;&nbsp;&nbsp;&nbsp;&lt;w2:gBody <span class="r_h_attribute_name_span">id</span>=<span class="r_h_attribute_value_span">"gBody1"</span> <span class="r_h_attribute_name_span">style</span>=<span class="r_h_attribute_value_span">""</span>&gt;

/* 예제 파일에서 사용하는 CSS 모음 */

/* 예제 공통  - 공백 표현 */
.P00000_preColumn > .w2grid_input {white-space: pre;}

/* P00029.xml class 제어 예제 */
.P00029_exam1 {border-color: #e40a0a; color: #e40a0a;}
.P00029_exam2 {border-color: #0070c0; color: #0070c0;}

/* P00145.xml WindowContainer의 선택된 윈도우 style */
.P00145.w2windowContainer .w2window_selected { border-color: MediumBlue;}
.P00145.w2windowContainer .w2window_selected .w2window_selected_header .w2window_header_title{ color: MediumBlue; }

/* P00186.xml 예제 */
.w2grid .P00186_exam1 {background-color: #F0E68C;}

/* P00189.xml 예제 */
.P00189_exam2 .w2grid_default_disabled {background-color: #808080 !important; color: #A9A9A9 !important;}

/* P00233.xml 예제 */
.w2grid .P00233_exam1 {background-color: #F0E68C !important; color: #DC143C !important;}

/* P00271.xml 모바일에서 예제 테스트를 위해 th 영역 크기 조절 */
.P00271.tbl.w2tb_adaptive_layout .w2tb_td:before {width: 70px !important;}
.P00271.tbl.w2tb_adaptive_layout .w2tb_td {padding-left: 76px !important;}